{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "plugin": ["opencode-gemini-auth@latest"],
  "model": "google/gemini-2.5-flash",
  "autoupdate": false,
  "tui": {
    "scroll_speed": 3,
    "scroll_acceleration": {
      "enabled": true
    },
    "diff_style": "auto"
  },

  "permission": {
    "edit": "ask",
    "read": {
      "*": "allow",
      "*.env": "deny",
      "*.env.*": "deny",
      "*.env.example": "allow"
    }
  },

  "server": {
    "port": 4096,
    "hostname": "0.0.0.0",
    "mdns": true,
    "cors": ["http://localhost:5173"]
  },

  "default_agent": "plan",

  "agent": {
    "code-reviewer": {
      "description": "Reviews code for best practices and potential issues",
      "model": "anthropic/claude-sonnet-4-5",
      "prompt": "You are a code reviewer. Focus on security, performance, and maintainability.",
      "tools": {
        "write": false,
        "edit": false,
      },
    },


  },

  "disabled_providers": ["openai", "gemini"],

  "provider": {

    "openrouter": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Openrouter CLI-Proxy ",
      "options": {
        "baseURL": "https://openrouter.4nd.xyz/v1",
        "apiKey": "{env:ANTHROPIC_API_KEY}",
        "headers": {
          "Authorization": "Bearer custom-token"
        }
      },
      "models": {
        "grok-code-fast-1": {
          "name": "CLI-Proxy - Grok Code Fast 1",
          "limit": {
            "context": 256000,
            "output": 4096
          }
        },
        "gpt-4.1": {
          "name": "CLI-Proxy - GPT-4.1",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-4o": {
          "name": "CLI-Proxy - GPT-4o",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5.2": {
          "name": "CLI-Proxy - GPT-5.2",
          "limit": {
            "context": 200000,
            "output": 4096
          }
        },
        "gpt-5.2-codex": {
          "name": "CLI-Proxy - GPT-5.2 Codex",
          "limit": {
            "context": 200000,
            "output": 4096
          }
        },
        "claude-haiku-4.5": {
          "name": "CLI-Proxy - Claude Haiku 4.5",
          "limit": {
            "context": 200000,
            "output": 4096
          }
        },
        "claude-opus-4.5": {
          "name": "CLI-Proxy - Claude Opus 4.5",
          "limit": {
            "context": 200000,
            "output": 4096
          }
        },
        "claude-sonnet-4.5": {
          "name": "CLI-Proxy - Claude Sonnet 4.5",
          "limit": {
            "context": 200000,
            "output": 4096
          }
        },
        "gemini-2.5-pro": {
          "name": "CLI-Proxy - Gemini 2.5 Pro",
          "limit": {
            "context": 1000000,
            "output": 4096
          }
        },
        "gemini-3-flash": {
          "name": "CLI-Proxy - Gemini 3 Flash",
          "limit": {
            "context": 1000000,
            "output": 4096
          }
        },
        "gemini-3-pro": {
          "name": "CLI-Proxy - Gemini 3 Pro",
          "limit": {
            "context": 1000000,
            "output": 4096
          }
        },
        "glm4.7": {
          "name": "CLI-Proxy - GLM 4.7",
          "limit": {
            "context": 128000,
            "output": 16384
          }
        },
        "kimi-k2": {
          "name": "CLI-Proxy - Kimi K2.5",
          "limit": {
            "context": 128000,
            "output": 16384
          }
        },
        "gemini-2.5-flash": {
          "name": "CLI-Proxy - Gemini 2.5 Flash",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "claude-opus-4.1": {
          "name": "CLI-Proxy - Claude Opus 4.1",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gemini-3-flash-preview": {
          "name": "CLI-Proxy - Gemini 3 Flash Preview",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5-mini": {
          "name": "CLI-Proxy - Gpt 5 Mini",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5.1-codex": {
          "name": "CLI-Proxy - Gpt 5.1 Codex",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gemini-2.5-flash-lite": {
          "name": "CLI-Proxy - Gemini 2.5 Flash Lite",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-oss-120b-medium": {
          "name": "CLI-Proxy - Gpt Oss 120B Medium",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "tab_flash_lite_preview": {
          "name": "CLI-Proxy - Tab_Flash_Lite_Preview",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "copilot-gpt5": {
          "name": "CLI-Proxy - Copilot Gpt5",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5.1-codex-max": {
          "name": "CLI-Proxy - Gpt 5.1 Codex Max",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gemini-3-pro-image": {
          "name": "CLI-Proxy - Gemini 3 Pro Image",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5.1-codex-mini": {
          "name": "CLI-Proxy - Gpt 5.1 Codex Mini",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "claude-sonnet-4": {
          "name": "CLI-Proxy - Claude Sonnet 4",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "claude-opus-4-5-thinking": {
          "name": "CLI-Proxy - Claude Opus 4_5 Thinking",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "claude-sonnet-4-5": {
          "name": "CLI-Proxy - Claude Sonnet 4.5",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5-codex": {
          "name": "CLI-Proxy - Gpt 5 Codex",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gpt-5.1": {
          "name": "CLI-Proxy - Gpt 5.1",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gemini-3-pro-high": {
          "name": "CLI-Proxy - Gemini 3 Pro High",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "gemini-3-pro-preview": {
          "name": "CLI-Proxy - Gemini 3 Pro Preview",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "claude-sonnet-4-5-thinking": {
          "name": "CLI-Proxy - Claude Sonnet 4_5 Thinking",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        },
        "oswe-vscode-prime": {
          "name": "CLI-Proxy - Oswe Vscode Prime",
          "limit": {
            "context": 128000,
            "output": 4096
          }
        }
      }
    },

    "google": {
      "models": {
        "gemini-2.5-flash": {
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 8192,
              "includeThoughts": true
            }
          }
        },
        "gemini-2.5-pro": {
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 8192,
              "includeThoughts": true
            }
          }
        },
        "gemini-3-flash-preview": {
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "high",
              "includeThoughts": true
            }
          }
        },
        "gemini-3-pro-preview": {
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "high",
              "includeThoughts": true
            }
          }
        }
      }
    }
  },

  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp"
    },
    "gh_grep": {
      "type": "remote",
      "url": "https://mcp.grep.app"
    },

    "sentry": {
      "type": "remote",
      "url": "https://mcp.sentry.dev/mcp",
      "enabled": false,
      "oauth": {}
    },

    "xcode": {
      "type": "local",
      "command": ["xcrun", "mcpbridge"],
      "enabled": true
    },

    "chrome-devtools": {
      "type": "local",
      "command": ["npx", "-y", "chrome-devtools-mcp@latest"],
      "enabled": false
    },

    "playwright": {
      "type": "local",
      "command": [
        "npx",
        "@playwright/mcp@latest"
      ],
      "enabled": false
    }
  },
  "formatter": false,

  "lsp": {
    "sourcekit-lsp": {
      "disabled": true
    }
  }
}
