{
  "$schema": "https://charm.land/crush.json",
  "options": {
    "attribution": {
      "trailer_style": "none",
      "generated_with": false
    },
    "skills_paths": [
      "~/.claude/skills/"
    ]
  },
  "mcp": {
    "gh_grep": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp",
      "disabled": true
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "disabled": true
    },
    "xcode": {
      "type": "stdio",
      "command": "xcrun",
      "args": [
        "mcpbridge"
      ],
      "disabled": true
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "disabled": true
    }
  },
  "lsp": {
    "typeprof": {
      "disabled": true
    },
    "solargraph": {
      "disabled": true
    },
    "rubocop": {
      "disabled": true
    },
    "sourcekit": {
      "command": "sourcekit-lsp",
      "root_markers": [
        "Package.swift",
        "."
      ],
      "filetypes": [
        "swift",
        "objective-c",
        "cpp"
      ],
      "disabled": true
    }
  },
